<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.abcapp.Notif.ui.notification.NotifFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_notif" >
        <action
            android:id="@+id/action_navigation_home_to_navigation_location"
            app:destination="@id/navigation_location" />

    </fragment>

    <fragment
        android:id="@+id/navigation_location"
        android:name="com.example.abcapp.Notif.ui.carparkSelector.CarparkFragment"
        android:label="@string/title_location"
        tools:layout="@layout/fragment_carparkk" >
        <action
            android:id="@+id/action_navigation_location_to_navigation_home"
            app:destination="@id/navigation_home" />
    </fragment>

</navigation>
